function sendcaptcha()
    local text = [[
        {
            "clientKey": "CAP-57E9353D6D192A1F001F94D3A5C4DFF4",
            "task": {
              "type": "ReCaptchaV2Task",
              "websiteURL": "https://www.growtopiagame.com/player/login?token=7Xc84tcjEcheQKXPQYAa%2FYQEQUBiXLgZBgkUI1Taj1lxAiw9xNbthghIDi1Z6VfVlMpZfyhtMmL0QtS5pCsnr8qX%2BgaEOfLPJ0Avq7QfLgKQuMIvI3qJQIPQzS4c31zZJxBrJYSZvVdZl8vX2vBdH8%2FZSONtT%2Bhw5uEDsZPB7cYo%2FeELgGSVnUi25BZMQ1q8Wpx5Zla7p58CrqGzYIHntzW48C6rewrzpyTMizNJMJforcNBUOSAMb95GoeXAluXYYAClGorepqQFGXc2C15vP5IwS6I0hAl5SF7ZU7fhFg%3D&platformID=2",
              "websiteKey": "6LdPe_4UAAAAAJ8vlXFg6CuLxZEluUNUtbU87ZGs",
              "isInvisible": false,
              "pageAction": "submit"
            }
        }          
    ]]

    ReqInfo = {}
    ReqInfo.url = "https://api.capsolver.com/createTask"
    ReqInfo.method = POST
    ReqInfo.json = true
    ReqInfo.postData = text
    x = httpReq(ReqInfo)
    if x.success then
        local response = x.body
        log(response)
        log("Response Http Status Code : ",x.httpCode)
        sleep(1000)
        local cleaned = response:gsub('[%s"]', '')
        log(cleaned)
        local resultId = cleaned:match('taskId:(.*),')
        log(resultId)
    else
        log("Request Failed Error Msg : ",x.failInfo)
    end
end

