resultId = 0

function sendcaptcha()
    local text = [[
        {
            "clientKey": "CAP-57E9353D6D192A1F001F94D3A5C4DFF4",
            "task": {
              "type": "ReCaptchaV2Task",
              "websiteURL": "https://www.growtopiagame.com/player/login?token=7Xc84tcjEcheQKXPQYAa%2FT6LU4jA7alJxK%2BvSbbh5laPgw%2Fegaa2iBnniYwRaSfA134JkyUEcf%2F9QacYhQMGGb6hJC5l80%2FXhpCJBsK%2B5hK66eb4%2BnIVF%2Fz%2B00m0OSFff0iQDy0vk%2Fv7mwnwtGW3S%2Bq0BWV%2FkhdhB8pwT2xcpVvKYPwYXcwTrlaoGTTOKoGxfTcIX%2F%2FfyMW9j%2FfpHEj8hJ30IsnJ3xxdUAOUA%2Bj3vymGtZtpxviULzClttf%2F61iBgPOTEJgB9StatZQRZRddLruSx%2BKdREredMjrOnJAt5k%3D&platformID=2",
              "websiteKey": "6LdPe_4UAAAAAJ8vlXFg6CuLxZEluUNUtbU87ZGs",
              "isInvisible": false,
              "pageAction": "submit"
            }
        }          
    ]]

    ReqInfo = {}
    ReqInfo.url = "https://api.capsolver.com/createTask"
    ReqInfo.method = POST
    ReqInfo.json = true
    ReqInfo.postData = text
    rekues1 = httpReq(ReqInfo)

    if rekues1.success then
        jancok = rekues1.body
        cleaned = jancok:gsub('[%s"]', '')
        resultId = cleaned:match('taskId:(.*),')
        log(jancok)
        log(resultId)
    else
        log("Request Failed Error Msg : ",x.failInfo)
    end
end

function getresult(idnya)
    local text = [[
        {
            "clientKey": "CAP-57E9353D6D192A1F001F94D3A5C4DFF4",
            "taskId": "]]..idnya..[["
        }          
    ]]

    ReqInfo = {}
    ReqInfo.url = "https://api.capsolver.com/getTaskResult"
    ReqInfo.method = POST
    ReqInfo.json = true
    ReqInfo.postData = text
    rekues2 = httpReq(ReqInfo)
    if rekues2.success then
        ngentod = rekues2.body
        log(ngentod)
    else
        log("Request Failed Error Msg : ",x.failInfo)
    end
end

