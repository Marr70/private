function sendcaptcha()
    local text = [[
        {
            "clientKey": "YOUR_API_KEY",
            "task": {
              //Required
              "type": "ReCaptchaV2Task",
              //Required
              "websiteURL": "https://www.google.com/recaptcha/api2/demo",
              //Required
              "websiteKey": "6Le-wvkSAAAAAPBMRTvw0Q4Muexq9bi0DJwx_mJ-",
              "isInvisible": false,
              // Optional
              "pageAction": "submit",
              //Optional
              "apiDomain": "",
              //Optional
              "userAgent": "",
              //Optional
              "cookies": [
                {
                  "name": "__Secure-3PSID",
                  "value": "AIKkIs3ch7YsxxxxYIzRqNZPGm60cdHozgwfUW1o8MF3kRcf8clJscTI6OtCqVpqNF8I88pLBJkUgQ"
                },
                {
                  "name": "__Secure-3PAPISID",
                  "value": "TKS1iVpGxYbxxxk0n2o/AytXQTb6RUALqxSEL"
                }
              ]
            }
        }          
    ]]

    ReqInfo = {}
    ReqInfo.url = "https://api.capsolver.com/createTask"
    ReqInfo.method = POST
    ReqInfo.json = true
    ReqInfo.postData = text
    x = httpReq(ReqInfo)
    if x.success then
        log("Response Body : ",x.body)
        log("Response Http Status Code : ",x.httpCode)
    else
        log("Request Failed Error Msg : ",x.failInfo)
    end
end

