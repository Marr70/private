              "isInvisible": false,
              "pageAction": "submit"
            }
        }          
    ]]

    ReqInfo = {}
    ReqInfo.url = "https://api.capsolver.com/createTask"
    ReqInfo.method = POST
    ReqInfo.json = true
    ReqInfo.postData = text
    x = httpReq(ReqInfo)
    if x.success then
